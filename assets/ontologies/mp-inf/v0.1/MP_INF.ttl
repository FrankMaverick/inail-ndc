INAIL:haNazione a owl:ObjectProperty ;
    rdfs:label "haNazione"@it ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf (
                    INAIL:Regione
                    [ a owl:Class ;
                        owl:unionOf (
                                [ a owl:Class ;
                                    owl:unionOf (
                                            INAIL:CittaMetropolitana
                                            INAIL:Provincia ) ]
                                [ a owl:Class ;
                                    owl:unionOf (
                                            INAIL:Comune
                                            [ a owl:Restriction ;
                                                owl:onProperty [ owl:inverseOf INAIL:haStatoDiSoggetto ] ;
                                                owl:someValuesFrom INAIL:Comune ] ) ]
                                [ a owl:Restriction ;
                                    owl:onProperty [ owl:inverseOf INAIL:haStatoDiSoggetto ] ;
                                    owl:someValuesFrom [ a owl:Class ;
                                            owl:unionOf (
                                                    INAIL:CittaMetropolitana
                                                    INAIL:Provincia ) ] ] ) ]
                    [ a owl:Restriction ;
                        owl:onProperty [ owl:inverseOf INAIL:haStatoDiSoggetto ] ;
                        owl:someValuesFrom INAIL:Regione ] ) ] ;
    rdfs:range INAIL:Nazione .

INAIL:haProvincia a owl:ObjectProperty ;
    rdfs:label "haProvincia"@it ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf (
                    INAIL:Comune
                    [ a owl:Restriction ;
                        owl:onProperty [ owl:inverseOf INAIL:haStatoDiSoggetto ] ;
                        owl:someValuesFrom INAIL:Comune ] ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf (
                    INAIL:CittaMetropolitana
                    INAIL:Provincia ) ] .

