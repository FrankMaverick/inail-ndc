INAIL:haRegione a owl:ObjectProperty ;
    rdfs:label "haRegione"@it ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf (
                    [ a owl:Class ;
                        owl:unionOf (
                                INAIL:CittaMetropolitana
                                INAIL:Provincia ) ]
                    [ a owl:Class ;
                        owl:unionOf (
                                INAIL:Comune
                                [ a owl:Restriction ;
                                    owl:onProperty [ owl:inverseOf INAIL:haStatoDiSoggetto ] ;
                                    owl:someValuesFrom INAIL:Comune ] ) ]
                    [ a owl:Restriction ;
                        owl:onProperty [ owl:inverseOf INAIL:haStatoDiSoggetto ] ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:unionOf (
                                        INAIL:CittaMetropolitana
                                        INAIL:Provincia ) ] ] ) ] ;
    rdfs:range INAIL:Regione .

