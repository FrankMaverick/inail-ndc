            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty inail:settoreCorrelatoMalattiaProfessionale ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty inail:subsettoreCorrelatoMalattiaProfessionale ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty inail:tipoDecisioneIstruttoriaAmministrativaMalattiaProfessionale ;
            owl:someValuesFrom rdfs:Literal ],
